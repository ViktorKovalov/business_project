<%= form_with model: @feedback_request, url: feedback_requests_path, local: true do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name, placeholder: "Your Name" %>
  </div>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email, placeholder: "Your Email" %>
  </div>
  <div>
    <%= form.label :phone_number %>
    <%= form.number_field :phone_number, placeholder: "Your Phone Number" %>
  </div>
  <div>
    <%= form.label :message %>
    <%= form.text_area :message, placeholder: "Your Message" %>
  </div>
  <%= form.submit 'Send' %>
<% end %>